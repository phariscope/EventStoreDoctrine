{
    "name": "phariscope/event-store-doctrine",
    "type": "library",
    "description": "Store event via Doctrine",
    "keywords": ["event","doctrine","store events","DDD"],
    "homepage": "https://github.com/phariscope/EventStoreDoctrine",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Frédéric Royet",
            "email": "frederic.royet@logipro.com",
            "homepage": "https://www.linkedin.com/in/frederic-royet-42568566/",
            "role": "Project Founder"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phariscope/event-store": "^1.0",
        "doctrine/orm": "^2.16",
        "symfony/cache": "^6.0|^6.3"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.27",
        "phpstan/phpdoc-parser": "^1.20",
        "phpstan/phpstan": "1.10.15",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Phariscope\\EventStoreDoctrine\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Phariscope\\EventStoreDoctrine\\Tests\\" : "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
